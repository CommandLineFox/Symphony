{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "member-ordering": false,
        "no-console": false,
        "no-constant-condition": true,
        "variable-name": {
                "options": [
                    "ban-keywords",
                    "check-format",
                    "allow-leading-underscore",
                    "allow-pascal-case"
                ]
        }
    },
    "rulesDirectory": [
        "node_modules/tslint-eslint-rules/dist/rules"
    ]
}